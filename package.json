{
    "name": "gitup",
    "displayName": "GitUp",
    "description": "Open project in GitUp",
    "version": "0.0.2",
    "publisher": "Braver",
    "repository": {
        "type": "git",
        "url": "https://github.com/braver/vscode-gitup.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#333333"
    },
    "activationEvents": [
        "onCommand:extension.opengitup"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.opengitup",
                "title": "Open in GitUp"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}